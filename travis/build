#!/bin/bash
echo "hellow"
if [[ "$TRAVIS_OS_NAME" = "windows" ]] ; then
	set
	echo on
	echo "$VS140COMNTOOLS" 
	dir "$VS140COMNTOOLS"
	
	echo ".. "
	echo dir "$VS140COMNTOOLS\\.."
	dir "$VS140COMNTOOLS\\.."
	echo dir "$VS140COMNTOOLS\\..\\.."
	dir "$VS140COMNTOOLS\\..\\.."
	echo "....."
	dir "$VS140COMNTOOLS\\..\\..\\VC\\"
	echo dir "$VS140COMNTOOLS\\..\\..\\VC\\vcvarsall.bat"
	dir "$VS140COMNTOOLS\\..\\..\\VC\\vcvarsall.bat"
	"$VS140COMNTOOLS\\..\\..\\VC\\vcvarsall.bat" x64
	msbuild /?
else
	set -ev
	pushd demo/noui && make clean && make && popd
	pushd demo/noui_c && make clean && make && popd
	pushd demo/noframes && make clean && make && popd
fi
